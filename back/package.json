{
  "name": "back",
  "version": "1.0.0",
  "main": "./server.ts",
  "author": "Adrien Guinet",
  "license": "ISC",
  "compilerOptions": {
    "module": "CommonJS"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./server.ts",
    "start": "nodemon ./server.ts",
    "prod": "npm run build && npm run start",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate",
    "typeorm:migration:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^14.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.39",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "@types/validator": "^13.6.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
